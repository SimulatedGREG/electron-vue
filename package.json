{
  "name": "electron-vue",
  "version": "0.1.0",
  "description": "An Electron & Vue.js quick starter",
  "scripts": {
    "build": "node -r babel-register tasks/release.js",
    "build:clean": "del builds/* !.gitkeep",
    "build:darwin": "cross-env PLATFORM_TARGET=darwin node -r babel-register tasks/release.js",
    "build:linux": "cross-env PLATFORM_TARGET=linux node -r babel-register tasks/release.js",
    "build:mas": "cross-env PLATFORM_TARGET=mas node -r babel-register tasks/release.js",
    "build:win32": "cross-env PLATFORM_TARGET=win32 node -r babel-register tasks/release.js",
    "dev": "node -r babel-register tasks/runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter app",
    "pack": "cross-env NODE_ENV=production webpack -p --progress --colors",
    "postinstall": "node -r babel-register tasks/postinstall.js",
    "vue:route": "node -r babel-register tasks/vue/route.js",
    "vuex:module": "node -r babel-register tasks/vuex/module.js"
  },
  "author": "Greg Holguin <simulatedgreg@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.6.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "devtron": "^1.2.1",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^1.2.5",
    "electron-rebuild": "^1.1.3",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^1.4.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.16.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.7.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^8.3.1",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {}
}
