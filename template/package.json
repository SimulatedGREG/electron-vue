{
  "name": "{{ name }}",
{{#if_eq builder 'packager'}}
  "productName": "{{ name }}",
{{/if_eq}}
  "version": "{{ appver }}",
  "author": "{{ author }}",
  "description": "{{ description }}",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    {{#if_eq builder 'packager'}}
    "build": "node .electron-vue/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
    {{else}}
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    {{/if_eq}}
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    {{#if e2e}}
    "e2e": "npm run pack && mocha test/e2e",
    {{/if}}
    {{#if eslint}}
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src{{#testing unit e2e}} test{{/testing}}",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src{{#testing unit e2e}} test{{/testing}}",
    {{/if}}
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    {{#testing unit e2e}}
    "test": "{{#unit}}npm run unit{{/unit}}{{#unit}}{{#e2e}} && {{/e2e}}{{/unit}}{{#e2e}}npm run e2e{{/e2e}}",
    {{/testing}}
    {{#if unit}}
    "unit": "karma start test/unit/karma.conf.js",
    {{/if}}
    "postinstall": "{{#if eslint}}npm run lint:fix{{/if}}"
  },
  {{#if_eq builder 'builder'}}
  "build": {
    "productName": "{{ name }}",
    "appId": "{{ appid }}",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
{{/if_eq}}
  "dependencies": {
    "vue": "^2.6.14"{{deps plugins}}
  },
  "devDependencies": {
    "ajv": "^8.6.0",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/register": "^7.13.16",
    "babel-loader": "^8.2.2",
    "cfonts": "^2.1.2",
    "chalk": "^4.1.1",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "del": "^6.0.0",
    "devtron": "^1.4.0",
    "electron": "^13.1.1",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    {{#if_eq builder 'packager'}}
    "electron-packager": "^15.2.0",
    "electron-rebuild": "^2.3.5",
    {{else}}
    "electron-builder": "^22.11.5",
    {{/if_eq}}
    {{#eslint}}
    "babel-eslint": "^10.1.0",
    "eslint": "^7.28.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.1.2",
    {{#if_eq eslintConfig 'standard'}}
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    {{/if_eq}}
    {{#if_eq eslintConfig 'airbnb'}}
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.23.4",
    {{/if_eq}}
    {{/eslint}}
    "mini-css-extract-plugin": "1.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    {{#if unit}}
    "inject-loader": "^4.0.1",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.2",
    "karma-electron": "^6.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    {{/if}}
    "listr": "^0.14.3",
    {{#if e2e}}
    "require-dir": "^1.0.0",
    "spectron": "^3.8.0",
    {{/if}}
    {{#testing unit e2e}}
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    {{/testing}}
    "node-loader": "^2.0.0",
    {{#if usesass}}
    "node-sass": "^6.0.1",
    "sass-loader": "^12.1.0",
    {{/if}}
    "style-loader": "^3.0.0",
    "terser-webpack-plugin": "5.1.3",
    "url-loader": "^4.1.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.9.7",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack-cli": "^4.7.2",
    "webpack": "^5.38.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.8.0"
  }
}
